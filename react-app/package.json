{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5",
    "web-vitals": "^1.0.1"
  },
  "type": "module",
  "scripts": {
    "start:server": "cd server && source sendgrid.env && ts-node-dev ./server.ts --respawn --transpileOnly && wait-on tcp:5000",
    "start:app": "react-scripts start",
    "start": "run-p --race start:server start:app",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-server": "cd ./server && source sendgrid.env && PORT=5001 jest ./*.test.ts",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sendgrid/mail": "7.4.6",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.17",
    "cors": "2.8.5",
    "jest": "26.6.0",
    "run-p": "^0.0.0",
    "string-sanitizer": "2.0.2",
    "ts-jest": "26.5.6"
  }
}